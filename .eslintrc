{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react",
        "babel"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "camelcase": 2,
        "curly": 2,
        "eqeqeq": 2,
        "no-empty-label":1,
        "brace-style": [
            2,
            "1tbs"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "space-infix-ops": 2,
        "no-param-reassign": 0,
        "prefer-spread": 2,
        "react/jsx-boolean-value": 2,
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-sort-props":0,
        "react/sort-comp": 0,
        "react/prop-types": [
            2,
            {
                "ignore": [
                    "children",
                    "className",
                    "style"
                ]
            }
        ]
    }
}
